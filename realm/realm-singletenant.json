{
  "id": "project-realm",
  "realm": "project-realm",
  "displayName": "Project Realm",
  "enabled": true,
  "sslRequired": "external",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "passwordPolicy": "length(8) and digits(1) and upperCase(1) and lowerCase(1) and notUsername(undefined) and hashIterations(27500)",
  "accessTokenLifespan": 600,
  "accessTokenLifespanForImplicitFlow": 600,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "offlineSessionIdleTimeout": 2592000,
  "accessCodeLifespan": 60,
  "accessCodeLifespanUserAction": 300,
  "accessCodeLifespanLogin": 1800,
  "actionTokenGeneratedByAdminLifespan": 43200,
  "actionTokenGeneratedByUserLifespan": 300,
  "requiredCredentials": ["password"],
  "otpPolicyType": "totp",
  "otpPolicyAlgorithm": "HmacSHA1",
  "otpPolicyInitialCounter": 0,
  "otpPolicyDigits": 6,
  "otpPolicyLookAheadWindow": 1,
  "otpPolicyPeriod": 30,
  "roles": {
    "realm": [
      {
        "name": "owner",
        "description": "Project Owner - Full access to all resources",
        "composite": false,
        "clientRole": false,
        "containerId": "project-realm"
      },
      {
        "name": "operator",
        "description": "Project Operator - Operational access to resources",
        "composite": false,
        "clientRole": false,
        "containerId": "project-realm"
      },
      {
        "name": "admin",
        "description": "Project Administrator - Administrative access with MFA required",
        "composite": false,
        "clientRole": false,
        "containerId": "project-realm"
      }
    ]
  },
  "clients": [
    {
      "clientId": "project-web",
      "name": "Project Web Application",
      "description": "Single Page Application (SPA) client",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "redirectUris": [
        "https://localhost:3000/*",
        "http://localhost:3000/*"
      ],
      "webOrigins": [
        "https://localhost:3000",
        "http://localhost:3000"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      },
      "protocolMappers": [
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "user.attribute": "foo",
            "access.token.claim": "true",
            "claim.name": "realm_access.roles",
            "id.token.claim": "true",
            "multivalued": "true"
          }
        }
      ]
    },
    {
      "clientId": "project-backend",
      "name": "Project Backend Service",
      "description": "Backend service for server-to-server communication",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "publicClient": false,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false,
      "secret": "changeme-backend-client-secret",
      "attributes": {
        "access.token.lifespan": "600"
      }
    },
    {
      "clientId": "project-mobile",
      "name": "Project Mobile Application",
      "description": "Mobile application client",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "redirectUris": [
        "com.example.app:/oauth2redirect/*"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    }
  ],
  "smtpServer": {
    "host": "mailhog",
    "port": "1025",
    "from": "noreply@project.local",
    "fromDisplayName": "Project Team",
    "ssl": false,
    "starttls": false,
    "auth": false
  },
  "eventsEnabled": true,
  "eventsListeners": ["jboss-logging"],
  "adminEventsEnabled": true,
  "adminEventsDetailsEnabled": true,
  "browserFlow": "browser",
  "registrationFlow": "registration",
  "directGrantFlow": "direct grant",
  "resetCredentialsFlow": "reset credentials",
  "clientAuthenticationFlow": "clients"
}